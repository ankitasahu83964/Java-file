import java.util.*;
public class pangram {
	public static void main (String[] args){
		Scanner sc=new Scanner(System.in);
	        String s=sc.next();	         	
	    	int[]arr=new int[26];
	    	for(int i=0;i<26;i++){
	    		arr[s.charAt(i)-'a']++;
	    	}
	    	for(int i:arr){
	    		if(i==0){
	    			System.out.println("not pangram");
	    			break;
	    	}
	       	System.out.println(" pangram");
	    	}
	  }        
}
